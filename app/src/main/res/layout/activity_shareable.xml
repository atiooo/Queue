<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/activity_shareable"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:theme="@android:style/Theme.Material"
    android:background="@color/windowBackground"
    tools:context="com.example.lingfeng.dopeaf1.ViewNavigation">
    <!-- tools:showIn="@layout/app_bar_navigation_main"> -->

    <android.support.design.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        tools:context="com.example.lingfeng.dopeaf1.Navigation"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">


        <android.support.v7.widget.RecyclerView
            android:id="@+id/rv_shareable"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="#424242" />


    </android.support.design.widget.CoordinatorLayout>
</RelativeLayout>
